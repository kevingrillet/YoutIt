{
  "manifest_version": 2,
  "name": "Yout it",
  "version": "1.3",
  "description": "Permet d'aller directement sur Yout.com depuis YouTube",
  "homepage_url": "https://github.com/kevingrillet/YoutIt",
  "background": {
    "scripts": [
      "js/browser-polyfill.min.js",
      "js/iconClick.js",
      "js/iconFromUrl.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/logo.png"
  },
  "icons": {
    "128": "img/logo.png"
  },
  "permissions": [
    "tabs"
  ]
}
